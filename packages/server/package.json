{
  "name": "@synxjs/server",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./hydration": {
      "types": "./dist/hydration.d.ts",
      "import": "./dist/hydration.mjs",
      "require": "./dist/hydration.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.mjs",
      "require": "./dist/api.js"
    },
    "./dev": {
      "types": "./dist/dev.d.ts",
      "import": "./dist/dev.mjs",
      "require": "./dist/dev.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "test": "vitest run",
    "test:watch": "vitest --silent=false --watch --coverage --ui",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@synxjs/core": "workspace:*",
    "@synxjs/reactivity": "workspace:*",
    "@synxjs/types": "workspace:*",
    "@synxjs/vdom": "workspace:*",
    "@vue/compiler-sfc": "^3.4.0",
    "chokidar": "^3.5.3",
    "css": "^3.0.0",
    "esbuild": "^0.20.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/css": "^0.0.37",
    "@types/ws": "^8.5.10",
    "web-streams-polyfill": "^4.0.0"
  }
}
