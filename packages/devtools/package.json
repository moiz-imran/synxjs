{
  "name": "@synxjs/devtools",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint \"src/**/*.ts*\"",
    "test": "vitest --silent=false --watch --coverage",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@synxjs/hooks": "workspace:*",
    "@synxjs/jsx-dev-runtime": "workspace:*",
    "@synxjs/jsx-runtime": "workspace:*",
    "@synxjs/store": "workspace:*",
    "@synxjs/types": "workspace:*",
    "clsx": "^2.1.1"
  },
  "devDependencies": {
    "@synxjs/vdom": "workspace:*",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1"
  }
}
